# Supabase Configuration (필수)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Configuration (필수)
DATABASE_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres

# JWT Configuration (필수)
JWT_SECRET=your-jwt-secret-key-here-please-change-this-in-production

# Next.js Configuration (개발용)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-here

# Vercel Analytics (선택사항)
VERCEL_ANALYTICS_ID=your-vercel-analytics-id

# Environment
NODE_ENV=development

# AI API Keys (선택사항 - 나중에 AI 기능 추가 시)
# OPENAI_API_KEY=your-openai-api-key
# GEMINI_API_KEY=your-gemini-api-key